#include <stdio.h>

main()
{

	char zeichen='d';
	int position=4;

	printf("\tWichtige Mitteilung:\n%c ist der %d.Buchstabe im Alphabet",zeichen,position);

	/*
        Alternative Schreibweise zur besseren Lesbarkeit:
        printf("Wichtige Mitteilung:\n");
        printf("%c ist der %d.Buchstabe im Alphabet",zeichen,position);
	*/
}
